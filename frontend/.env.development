# Copy this file to .env.development or .env.local
# Vite Environment Variables

# Backend URL (default: http://localhost:8001)
VITE_BACKEND_URL=http://localhost:8001

# Default API Key for development
VITE_API_KEY=sk-default

# Auto-authenticate on startup (true/false)
VITE_AUTO_AUTHENTICATE=true

# Debug mode (true/false)
VITE_DEBUG=true

# Application title
VITE_APP_TITLE=LLM Proxy Dashboard

# WebSocket reconnection interval (milliseconds)
VITE_WS_RECONNECT_INTERVAL=5000
